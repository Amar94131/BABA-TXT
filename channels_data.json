import json

def read_channels_data():
    try:
        with open("channels.json", "r") as f:
            content = f.read().strip()  # फाइल के कंटेंट को पढ़ें और खाली स्थान हटा दें
            if not content:  # अगर कंटेंट खाली है
                print("फाइल खाली है, डिफॉल्ट डेटा लोड किया जा रहा है।")
                return {"channels": []}  # डिफॉल्ट चैनल्स लिस्ट
            return json.loads(content)  # यदि कंटेंट मौजूद है, तो JSON पार्स करें
    except (json.JSONDecodeError, FileNotFoundError) as e:
        print(f"त्रुटि: {e}. फाइल को लोड नहीं किया जा सका।")
        return {"channels": []}  # फाइल न होने पर भी डिफॉल्ट डेटा लौटाएं

# फाइल से डेटा पढ़ने की कोशिश करें
channels = read_channels_data()
print(channels)
